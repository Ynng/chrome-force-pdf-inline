{
    "background": {
        "scripts": [
            "background.js"
        ],
	"persistent": false
    },
    "content_security_policy": "script-src 'self'; object-src 'self'",
    "description": "Force PDFs to render inline. May cause filename to be lost.",
    "manifest_version": 2,
    "name": "Render PDFs Inline",
    "permissions": [
        "declarativeWebRequest",
        "*://*/*"
    ],
    "version": "2014.2.11"
}